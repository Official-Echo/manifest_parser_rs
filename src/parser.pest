COMMENT    = { "#" ~ (!NEWLINE ~ ANY)* ~ (NEWLINE | EOI) }

section_name = { (!"]" ~ ANY)+ }
key          = { (ASCII_ALPHANUMERIC | "_" | "-")+ }
value        = { (!NEWLINE ~ ANY)+ }

section   = { "[" ~ section_name ~ "]" }
key_value = { key ~ "=" ~ value }

manifest = {
    SOI ~ (COMMENT | section ~ (COMMENT | key_value)*)* ~ EOI // equals to ^(COMMENT | section(COMMENT | key_value)*)*$
}
